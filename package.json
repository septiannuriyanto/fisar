    "@supabase/supabase-js": "^2.49.4",
    "@whiskeysockets/baileys": "^6.17.16",
    "axios": "^1.10.0",
    "baileys": "^6.7.16",
    "basic-ftp": "^5.0.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "emailjs-com": "^3.2.0",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "node-mailjet": "^6.0.8",
    "nodemailer": "^7.0.3",
    "pino": "^9.7.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "redux": "^5.0.1",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
