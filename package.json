  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/redux": "^3.6.31",
    "@types/winston": "^2.4.4",
    "@types/xlsx": "^0.0.35",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "typescript": "^5.8.3"
