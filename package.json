{
  "name": "fisar",
  "version": "1.0.0",
  "author": "Septian Nuriyanto",
  "main": "main.js",
  "build": {
    "appId": "com.ritation.bot",
    "productName": "Ritation Bot",
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "renderer.html",
      "icon.ico"
